#include<stdio.h>
main()
{
	double first,second,temp;
	printf("enter first number");
	scanf("%lf",&first);
	printf("enter second number");
	scanf("%lf",&second);
	temp=first;
	first=second;
	second=temp;
	printf("after swapping,first number is %lf",first);
	printf("after swapping,second number is %lf",second);
	return 0;
}
